# Configuração do HikCentral Automation
# Copie este arquivo para .env e preencha com suas configurações

# URL do servidor HikCentral
HIKCENTRAL_URL=http://45.4.132.189:3389

# Credenciais de acesso
HIKCENTRAL_USERNAME=admin
HIKCENTRAL_PASSWORD=sua_senha_aqui

# Configurações de automação
DEBUG_MODE=true
HEADLESS_MODE=false
SIMULATION_MODE=false

# Delays e timeouts (em segundos)
LOGIN_DELAY=3
PAGE_LOAD_DELAY=5
ELEMENT_WAIT_TIMEOUT=10
HUMAN_DELAY_MIN=0.05
HUMAN_DELAY_MAX=0.15

# Configurações do Chrome
CHROME_HEADLESS=false
CHROME_WINDOW_SIZE=1920x1080
CHROME_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Logs e debug
LOG_LEVEL=DEBUG
SAVE_SCREENSHOTS=true
SCREENSHOT_DIR=screenshots
LOG_FILE=logs/automation.log

# Configurações de retry
MAX_RETRY_ATTEMPTS=3
RETRY_DELAY=1

# Configurações de dados de teste
TEST_VISITOR_NAME=João Silva Teste
TEST_VISITOR_CPF=12345678901
TEST_VISITOR_PHONE=31999999999
TEST_VISITOR_EMAIL=joao@teste.com
TEST_VISITOR_PLACA=ABC1234 